<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.example.eric.oscar.activity.OMerchantListActivity">

    <ListView
        android:id="@+id/merchantCateList"
        android:layout_width="@dimen/dp_58"
        android:layout_height="match_parent"
        android:background="@color/o_merchant_list_bg"
        android:divider="@color/o_line_bg"
        android:dividerHeight="@dimen/dp_1">

    </ListView>

    <ListView
        android:id="@+id/merchantList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/merchantCateList"
        android:divider="@color/o_line_bg"
        android:dividerHeight="@dimen/dp_1">

    </ListView>
</RelativeLayout>
