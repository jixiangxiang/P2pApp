<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eric.oscar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.OMainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OLoginActivity"
            android:label="@string/title_activity_ologin" />
        <activity
            android:name=".activity.ORegistActivity"
            android:label="@string/title_activity_oregist"
            android:parentActivityName=".common.BaseActivity" />
        <activity
            android:name=".activity.OModifyLoginPwdActivity"
            android:label="@string/title_activity_omodify_login_pwd" />
        <activity
            android:name=".activity.OSetPayPwdActivity"
            android:label="@string/title_activity_oset_pay_pwd" />
        <activity
            android:name=".activity.OResetPwdActivity"
            android:label="@string/title_activity_oreset_pwd" />
        <activity
            android:name=".activity.OChangePhoneActivity"
            android:label="@string/title_activity_ochange_phone" />
        <activity
            android:name=".activity.OPhoneCaptchaActivity"
            android:label="@string/title_activity_ochange_phone" />
        <activity
            android:name=".activity.OAccountActivity"
            android:label="@string/title_activity_oaccount" />
        <activity
            android:name=".activity.OFundsRecordActivity"
            android:label="@string/title_activity_ofunds_record"></activity>
    </application>

</manifest>
