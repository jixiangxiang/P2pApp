<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:windowExitAnimation">@android:anim/slide_out_right</item>
        <item name="android:windowEnterAnimation">@android:anim/slide_in_left</item>
        <!--Status bar color-->
        <item name="colorPrimaryDark">@color/p_bottom_bar_color</item>
    </style>

    <style name="Start_Theme_Default" parent="AppTheme">
        <item name="android:windowBackground">@mipmap/p_start_bg</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="dialog_animations" parent="android:Animation">
        //进入时的动画
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        //退出时的动画
        <item name="android:windowExitAnimation">@anim/push_top_out</item>
    </style>

    <style name="alert_dialog_animations" parent="android:Animation">
        //进入时的动画
        <item name="android:windowEnterAnimation">@anim/push_top_in</item>
        //退出时的动画
        <item name="android:windowExitAnimation">@anim/push_top_out</item>
        <item name="android:interpolator">@android:anim/bounce_interpolator</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

